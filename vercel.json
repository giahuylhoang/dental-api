{
  "buildCommand": "echo 'Build complete'",
  "outputDirectory": ".",
  "framework": null,
  "rewrites": [{"source": "/(.*)", "destination": "/api"}],
  "env": {"PYTHONUNBUFFERED": "1"},
  "functions": {
    "api/**/*.py": {
      "excludeFiles": "**/tests/**,**/__pycache__/**,**/*.md,**/scripts/**,**/test_*.py,**/*.db,.venv/**"
    }
  }
}
